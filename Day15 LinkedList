 public static  Node insert(Node head,int data) {
        //Complete this method 
        Node temp=new Node(data);
        Node curr=head; 
        if(head==null) 
            {
            head=temp; 
            return temp;   
        }
        
        else
        while(curr.next!=null)
            {
              curr=curr.next;   
            }
        curr.next=temp;
        return head; 
    }
    
