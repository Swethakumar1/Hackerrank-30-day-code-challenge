public class Solution {
    Stack<Character> st=new Stack<Character>();  
    Queue<Character> qu=new LinkedList<Character>(); 
    
    void pushCharacter(char ch){
        st.push(ch); 
    }
    
    void enqueueCharacter(char ch){
        qu.add(ch);
    }
    
    char popCharacter() {
        return st.pop(); 
    }
    
    char dequeueCharacter(){
        return qu.remove(); 
    }
    
    
